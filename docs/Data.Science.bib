	pages = {e02385--16},
	file = {PubMed Central Full Text PDF:C\:\\Users\\natal\\Zotero\\storage\\KLUAAP6H\\Hossain et al. - 2017 - A Phase I, Randomized, Double-Blinded, Placebo- an.pdf:application/pdf},
