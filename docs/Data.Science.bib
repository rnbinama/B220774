	urldate = {2024-10-26},
	journal = {Annals of Hepatology},
	author = {Cavalcante, Lourianne Nascimento and Porto, Jun and Mazo, Daniel and Longatto-Filho, Adhemar and Stefano, José Tadeu and Lyra, Andre Castro and Carrilho, Flair Jose and Reis, Rui Manuel and Alves, Venâncio A. F. and Sanyal, Arun J. and Oliveira, Claudia P},
