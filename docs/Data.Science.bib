	pmcid = {PMC6618811},
	keywords = {Animals, Green Fluorescent Proteins, Mice, Mice, Transgenic, Optogenetics, Afferent Pathways, Avoidance Learning, Cells, Cultured, Channelrhodopsins, Female, Ganglia, Spinal, Hyperalgesia, In Vitro Techniques, Male, Mice, Inbred C57BL, Morphine, NAV1.8 Voltage-Gated Sodium Channel, Pain, Pain Threshold, Receptors, Purinergic P2X3, Sensory Receptor Cells, tau Proteins, Valine, Wakefulness},
	pages = {18631--18640},
