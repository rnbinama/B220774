	year = {2011},
	keywords = {Multiple sclerosis, Cytokines, Flow cytometry, Fluorescent bead-based immunoassay, Memory cells, T-cells},
	pages = {135--141},
	file = {ScienceDirect Snapshot:C\:\\Users\\natal\\Zotero\\storage\\CA7CNUSB\\S0022510X1000420X.html:text/html},
}

@article{barres_axonal_1999,
	title = {Axonal {Control} of {Oligodendrocyte} {Development}},
