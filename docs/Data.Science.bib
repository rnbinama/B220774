	keywords = {Humans, Staphylococcus aureus, Biological Evolution, Genome, Bacterial, methicillin-resistant S. aureus (MRSA), Methicillin-Resistant Staphylococcus aureus, molecular characterization, molecular epidemiology, Molecular Epidemiology, Molecular Typing, MRSA evolution, MRSA origin, MRSA typing, SCCmec, Staphylococcal Infections},
	pages = {e00020--18},
	file = {Full Text:C\:\\Users\\natal\\Zotero\\storage\\FAQTBU37\\Lakhundi and Zhang - 2018 - Methicillin-Resistant Staphylococcus aureus Molec.pdf:application/pdf},
}

